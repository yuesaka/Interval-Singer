            android:name="eecs499.eartrainer.intervalsinger.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="eecs499.eartrainer.intervalsinger.AppPreferences"></activity>
    </application>
 	<uses-permission android:name="android.permission.RECORD_AUDIO" />
</manifest>